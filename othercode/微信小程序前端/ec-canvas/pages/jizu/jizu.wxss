/* pages/机组/jizu.wxss */
.container{
  width: 100%;
  display: flex;
}

.title{
  width: 100%;
  height: 700px;  
  text-align: center;
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  font-style:oblique;
  font-weight:bolder;
  font-size:25px;
  color:rgb(25, 0, 255)
}

.divLine1{
  background: rgb(0, 0, 0);
  width: 100%;
  height: 5rpx;
}

.history-table-wrap{
  position:relative;
  width: 668rpx;
  height: 500rpx;
  left: 50%;
  margin-left: -750rpx;
  top: 70rpx;
  overflow-y: scroll;
  overflow-x: hidden;
}
/* 表格代码   */
.table{
  border:1px solid #dadada;
  border-right:0;
  border-bottom: 0;
  width: 98%;
  margin-left: 1%;
}
.tr{
  width:100%;
  display: flex;
  justify-content: space-between;
}
.th,.td{
  padding: 10px;
  border-bottom: 1px solid #dadada;
  border-right: 1px solid #dadada;
  text-align: center;
  width: 100%;
}
.th1,.th2,.td1,.td2{
  width: 70%;
}
.th{
  font-weight: 800;
  background-color: #000000;
  font-size: 28rpx;
  color: #ffffff;
}
.td1,.td2{
  font-size: 25rpx;
  color: #000000;
}

.td3{
   font-size: 25rpx;
   color: #2015c4;
}



.text1{
  width: 100%;
  text-align: center;
  font-style: normal;
  font-weight: bold;
  font-size:20px;
}


